import java.util.Scanner;

public class IntegerToHex {
	public static void main(String[] args){
		Scanner sc = new Scanner(System.in);
		int one = sc.nextInt();
		int two = sc.nextInt();
		
		one = Integer.valueOf(one, 16);
		two = Integer.valueOf(two, 16);
		System.out.println(one + ""+ two);
		
	}
}
